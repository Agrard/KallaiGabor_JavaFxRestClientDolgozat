<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane hgap="20.0" prefHeight="480.0" prefWidth="640.0" vgap="20.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="hu.petrik.javafxrestclientdolgozat.UpdateAthleteController">
    <columnConstraints>
        <ColumnConstraints halignment="RIGHT" minWidth="75.0" prefWidth="100.0" />
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
    </columnConstraints>
    <rowConstraints>
      <RowConstraints minHeight="10.0" prefHeight="30.0" />
        <RowConstraints minHeight="10.0" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" valignment="TOP" vgrow="SOMETIMES" />
    </rowConstraints>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <children>
        <Label text="Active:" GridPane.rowIndex="0" />
        <Label text="Name:" GridPane.rowIndex="1" />
        <Label text="Age" GridPane.rowIndex="2" />
        <Label text="Home" GridPane.rowIndex="3" />
        <Label prefHeight="17.0" prefWidth="36.0" GridPane.rowIndex="0" />
        <CheckBox fx:id="activeField" centerShape="" GridPane.columnIndex="1" GridPane.rowIndex="0" />
        <TextField fx:id="nameField" GridPane.columnIndex="1" GridPane.rowIndex="1" />
        <Spinner fx:id="ageField" prefWidth="10000.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
        <TextField fx:id="homeField" GridPane.columnIndex="1" GridPane.rowIndex="3" />
        <Button fx:id="updateButton" mnemonicParsing="false" onAction="#updateClick" prefWidth="10000.0" text="Update" GridPane.columnSpan="2" GridPane.rowIndex="4" />
    </children>
</GridPane>
